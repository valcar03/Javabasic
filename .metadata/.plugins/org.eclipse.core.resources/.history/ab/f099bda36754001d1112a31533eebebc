package cl.generation.f20221025;

import java.util.ArrayList;

public class ArregloDin치mico {

	public static void main(String[] args) {
		// Arraylist
		// es un arreglo din치mico con la ventaja que no debes necesariamente declarar el tama침o
		//ArrayList<TipoDeDato> nombre_array = ArrayList<TipoDato>();
		//para importar todo de una es control+shift+o
		ArrayList<String> grupo2 = new ArrayList<String>();
		//agregar valor
		grupo2.add("Vale");
		grupo2.add("Diego");
		grupo2.add("Ariel");
		grupo2.add("Fran");
		grupo2.add("Nathalie");
		System.out.println(grupo2);
		//tama침o arraylist
		System.out.println(grupo2.size());
	}

}
